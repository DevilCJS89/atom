<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:macro name="atlascar2_calibration" params="prefix">

        <!--<sensor name="wide_stereo_left" update_rate="20.0">-->
            <!--<parent link="car_center"/>-->
