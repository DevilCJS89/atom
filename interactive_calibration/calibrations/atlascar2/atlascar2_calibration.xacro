        <sensor name="${prefix}frontal_camera" update_rate="20">
            <parent link="${prefix}frontal_camera"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <calibration_parent link="${prefix}car_center"/>
            <calibration_child link="${prefix}frontal_camera"/>
        </sensor>

        <!--<sensor name="${prefix}top_camera" update_rate="20">-->
            <!--<parent link="${prefix}top_camera_optical"/>-->
