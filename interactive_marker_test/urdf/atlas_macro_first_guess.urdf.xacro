  <joint name="base_link_to_frontal_laser_right" type="fixed">
    <origin xyz="-0.6 0.0 0.0" rpy="0.0 0.0 -2.09439510239"/>
    <parent link="base_link"/>
    <child link="frontal_laser_right"/>
  </joint>
  <link name="tablier_marker"/>
  <joint name="frontal_camera_optical_to_tablier_marker" type="fixed">
    <origin xyz="-0.0515 -0.1984 -0.8176" rpy="3.64159265359 -0.2 -1.57079632679"/>
    <parent link="frontal_camera_optical"/>
    <child link="tablier_marker"/>
