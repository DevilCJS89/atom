            <!--<calibration_parent link="${prefix}car_center"/>-->
            <!--<calibration_child link="${prefix}top_camera"/>-->
        <!--</sensor>-->

        <sensor name="${prefix}front_laser" update_rate="20">
            <parent link="${prefix}front_laser"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <calibration_parent link="${prefix}car_center"/>
            <calibration_child link="${prefix}front_laser_mount_link"/>
        </sensor>

        <sensor name="${prefix}left_laser" update_rate="20">
            <parent link="${prefix}left_laser"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <calibration_parent link="${prefix}car_center"/>
            <calibration_child link="${prefix}left_laser"/>
        </sensor>

        <sensor name="${prefix}right_laser" update_rate="20">
            <parent link="${prefix}right_laser"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <calibration_parent link="${prefix}car_center"/>
            <calibration_child link="${prefix}right_laser"/>
        </sensor>


    </xacro:macro>

